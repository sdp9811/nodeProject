<html>
    <head>
        <title><%=title %></title>
        <link   rel="stylesheet" href="/Style.css">
        <%- include('../Includes/header.ejs') %>
    </head>
    <body>
        <%- include('../Includes/navigation.ejs') %>
        
        <%if(data.length>0){%>
            
                <table >
                    <th colspan="3">Product Details </th>
                 
                    <tr >
                        <th>Product Id </th>
                        <th>Product Name</th>
                        <th>Price</th>
                    </tr>
                    <%for(let val of data){%>
                    <tr>
                    <td>  <%=val.prodId %> </td>
                    <td> <%=val.prodName %> </td>
                    <td>   <%=val.prodPrice %></td>
                    </tr>
                    <%}%>
                </table> 
                
                <hr>
            
            <%}else{%>
                <h1> data not found</h1>
                <%}%>
                <footer>
                    <p class="footer-style"> copyright @ copy:node developer.com</p>
                </footer>

    </body>
</html>